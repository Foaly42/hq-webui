string sProgramId; object oProgram; Write(\"{\"); foreach (sProgramId, dom.GetObject(ID_PROGRAMS).EnumUsedIDs()) { oProgram = dom.GetObject(sProgramId); if(oProgram != null) { Write(\"'program':{id:'\" # sProgramId #\"',active:'\" # oProgram.Active() # \"',\") Write(\"timestamp:'\" # oProgram.ProgramLastExecuteTime().ToInteger() #\"',name:'\"); Write( oProgram.Name() ); Write(\"',description:'\"); Write(oProgram.PrgInfo()); Write(\"'}\"); } } Write(\"}\"); 